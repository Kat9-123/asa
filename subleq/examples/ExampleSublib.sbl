; This is how Sublang could should be written, making extensive use of macros
!PrintStdLib p_string
!Halt


p_string -> &"Hello, Sublang!\n"
#sublib
#sublib/Control

**
    Or using the standard lib
**
@PrintStdLib P_STRING? {
    != p_local P_STRING?
    !=0 char
    
    !Loop {
        !DerefAndCopy p_local char
        !IfFalse char {
            !Break
        }
        !IO -= char
        !Inc p_local
    }
}