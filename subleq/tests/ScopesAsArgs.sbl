


W W .main

@Repeat l_times? s_body? {
    Z -= Z
    Z -= L_TIMES
    i -= Z

    .loop ->
        s_body?

        i -= ONE .fin
        W -= W .loop

    i -> 0
    L_TIMES -> l_times?
    .fin ->
}


@Flip s_body1? s_body2? {
    s_body2?

    s_body1?


}

@Skip
[
    &2
]

W -> 0
Z -> 0
A -> 'A'
B -> 'B'
ONE -> 1
LINE_BREAK -> 0xA


.main -> {
    !Repeat 2 {
        !Repeat 3 {
            !Flip {
                0xFFFF -= A
            } {
                0xFFFF -= B
                W W !Skip
                0xDEAD
            }
        }
        -1 -= LINE_BREAK
    }

    W -= W 0xFFFF
}





