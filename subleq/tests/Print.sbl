W -= W _loop



@Clear a? [
    a?      -= a?
]

@SubLit a? l_b? {
    a?      -= literal_pointer
    W       -= W             _fin
    literal_pointer -> l_b?
    _fin ->
}


_loop ->
    !Clear iChar
    !Clear char

    ptr -a>
    iChar   -= string ; 0 -= 0
    char    -= iChar
    char    -= Z            0xFFFF
    0xFFFF  -= char

    !SubLit ptr 0xFFFF
    Z       -= Z _loop



char -> 0
iChar -> 0

W -> 0 \ Z -> 0
nOne -> 0xFFFF
string -> "(\\'Hello, world!;\\')" 0






