!J .main
#Sublib
#Sublib/Control

THREE -> 3
i -> 3
p -> 0
TWO -> 2
a -> 'A'
LINEBREAK -> '\n'
.main ->
    !While::Neq p TWO {
        !Cpy THREE i
        !While::Ge i ZERO { ;::Ge i ZERO {
            !If::Neq SNEG ONE {
                !IO -= a
            }

            !Dec i
        }
        !IO -= LINEBREAK
        !Inc p
    }

    !Halt