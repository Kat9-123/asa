#Sublib/Symbols.sbl

@Halt [
    W -= W  0xFFFF
]


@Jump _address? [
    W -= W  _address?
]

@J _address? [!Jump _address?]

@Clear a? [
    a?      -= a?
]

@SubLit a? b? [
    a?      -= literal_pointer
    W       -= W             _fin
    literal_pointer -> b?
    _fin ->
]

@Inc a? [
    a? -= N_ONE
]