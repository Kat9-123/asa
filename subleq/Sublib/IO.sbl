#Sublib/Symbols.sbl
#Sublib/Base.sbl

@IO [0xFFFF]


@IO::Print string [{
    !J _loop
    char -> 0
    inv_char -> 0

    _loop ->
        ptr -a>
        inv_char    -= string ; 0 -= 0
        char        -= inv_char
        char        -= ZERO            _fin  ; Null zo we fin

        !IO         -= char

        !Inc ptr

        !Clr inv_char
        !Clr char
        Z       -= Z _loop


    _fin ->

}]